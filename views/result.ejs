<h3><%= query %></h3>
<h6><%= data.latitude %>, 
<%= data.longitude %> </h6>
<% const now = new Date() %>
<% now.setUTCMilliseconds(data.daily.data[1].time); %> 

<p> <%= now.getFullYear(); %> </p>


<div class="row">
<div class="col s12 m8 offset-m2">
  <div class="card large white darken-1">
    <div class="card-content black-text">
      <div class="row">
          <span class="card-title left"> <%= data.currently.icon %>   </span>
          <span class="card-title right"> <%= data.currently.temperature %>   </span>
      </div>
      <div class="row">
        <p> <%= data.daily.summary %>  </p>
      </div>
      <div class="row">
      
      </div>
    

    </div>
  </div>
</div>
</div>






<div class="card">
<div class="card-content">
  <p> <%= data.daily.summary %>  </p>
</div>
<div class="card-tabs">
  <ul class="tabs tabs-fixed-width">
    <li class="tab"><a class="active" href="#day1">Today</a></li>
    <li class="tab"><a href="#day2">Tomorrow</a></li>
    <li class="tab"><a href="#day3">The Next Day</a></li>
  </ul>
</div>
<div class="card-content grey lighten-4">
  <div id="day1">Today</div>
  <div id="day2">Tomorrow</div>
  <div id="day3">The Next Day</div>
</div>
</div>